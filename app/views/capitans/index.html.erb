<p id="notice"><%= notice %></p>

<h1>Listing Capitans</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Departamento</th>
      <th>Funcion</th>
      <th>Plata</th>
      <th>Team</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @capitans.each do |capitan| %>
      <tr>
        <td><%= capitan.nombre %></td>
        <td><%= capitan.departamento %></td>
        <td><%= capitan.funcion %></td>
        <td><%= capitan.plata %></td>
        <td><%= capitan.team %></td>
        <td><%= link_to 'Show', capitan %></td>
        <td><%= link_to 'Edit', edit_capitan_path(capitan) %></td>
        <td><%= link_to 'Destroy', capitan, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Capitan', new_capitan_path %>
