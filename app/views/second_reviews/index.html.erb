<p id="notice"><%= notice %></p>

<h1>Listing Second Reviews</h1>

<table>
  <thead>
    <tr>
      <th>Team</th>
      <th>Advantage</th>
      <th>Q7</th>
      <th>Q8</th>
      <th>Q9</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @second_reviews.each do |second_review| %>
      <tr>
        <td><%= second_review.team %></td>
        <td><%= second_review.advantage %></td>
        <td><%= second_review.q7 %></td>
        <td><%= second_review.q8 %></td>
        <td><%= second_review.q9 %></td>
        <td><%= link_to 'Show', second_review %></td>
        <td><%= link_to 'Edit', edit_second_review_path(second_review) %></td>
        <td><%= link_to 'Destroy', second_review, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Second review', new_second_review_path %>
