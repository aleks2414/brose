<p id="notice"><%= notice %></p>

<h1>Listing First Reviews</h1>

<table>
  <thead>
    <tr>
      <th>Team</th>
      <th>Challenge</th>
      <th>Q1</th>
      <th>Q2</th>
      <th>Q3</th>
      <th>Q4</th>
      <th>Q5</th>
      <th>Q6</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @first_reviews.each do |first_review| %>
      <tr>
        <td><%= first_review.team %></td>
        <td><%= first_review.challenge %></td>
        <td><%= first_review.q1 %></td>
        <td><%= first_review.q2 %></td>
        <td><%= first_review.q3 %></td>
        <td><%= first_review.q4 %></td>
        <td><%= first_review.q5 %></td>
        <td><%= first_review.q6 %></td>
        <td><%= link_to 'Show', first_review %></td>
        <td><%= link_to 'Edit', edit_first_review_path(first_review) %></td>
        <td><%= link_to 'Destroy', first_review, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New First review', new_first_review_path %>
